{
  "build": {
    "beforeDevCommand": "cd firesquare-launcher-ui && trunk serve",
    "beforeBuildCommand": "cd firesquare-launcher-ui && trunk build --release",
    "devPath": "http://localhost:1420",
    "distDir": "../firesquare-launcher-ui/dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "firesquare",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "category": "AdventureGame",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "xyz.frsqr.launcher",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEQ3ODgxMURCOTRGMjc1MzcKUldRM2RmS1UyeEdJMThSemdrS0QrS3ZnbnFnOURsTjVTaVhrRjkvdUlPTXhhYndMNlZ5OStRU1IK",
      "endpoints": [
        "https://api.frsqr.xyz/distibution/update_check/{{target}}/{{current_version}}"
      ]
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "firesquare",
        "width": 800
      }
    ]
  }
}
